{
    "name": "hexanet/monolog-extra-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle with extra processors and logger to log request/response",
    "keywords": ["log", "logging"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
          "Hexanet\\Common\\MonologExtraBundle\\": "src"
        }
    },
    "authors": [
        {
            "name": "Floran Brutel",
            "email": "f.brutel@hexanet.fr"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "symfony/security": "^3.0 || ^4.0",
        "symfony/config":"^3.0 || ^4.0",
        "symfony/http-foundation": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/http-kernel": "^3.0 || ^4.0",
        "symfony/monolog-bundle": "^2.8 || ^3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5|^3.0",
        "phpstan/phpstan": "^0.8.0"
    },
    "suggest": {
        "symfony/security": "^3.0 || ^4.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}
