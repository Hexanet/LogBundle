{
    "name": "hexanet/monolog-extra-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle with extra processors and logger to log request/response",
    "keywords": ["log", "logging"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
          "Hexanet\\Common\\MonologExtraBundle\\": "src"
        }
    },
    "authors": [
        {
            "name": "Floran Brutel",
            "email": "f.brutel@hexanet.fr"
        }
    ],
    "require": {
        "symfony/security": "~2.3|~3.0",
        "symfony/config": "~2.3|~3.0",
        "symfony/http-foundation": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/http-kernel": "~2.3|~3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5|^3.0"
    },
    "suggest": {
    },
    "config": {
        "bin-dir": "bin"
    }
}
